<fx:root fx:id="card" type="Pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="card_label" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutY="125.0" prefHeight="20.0" prefWidth="100.0" style="-fx-background-color: #c2c2c2; -fx-border-color: black;" text="LV 0 / EXP 0">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
      <ImageView fx:id="card_image" fitHeight="140.0" fitWidth="100.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</fx:root>
